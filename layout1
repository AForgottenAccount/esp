-- Base Misc
local Button = Section2:AddButton("Enable/Disable Mines", function()
	game:GetService("ReplicatedStorage").ToggleMines:InvokeServer()
end)
local Button = Section2:AddButton("Clear Base", function()
	game:GetService("ReplicatedStorage").DestroyAll:InvokeServer()
end)
local Button = Section2:AddButton("Rebirth", function()
	game:GetService("ReplicatedStorage").Rebirth:InvokeServer()
end)


local Toggle = Section2:AddToggle("Toggle Auto Rebirth & Auto Load", {flag = "Toggle_Flag", default = false}, function(bool)
   shouldLoop2 = bool
end)
while wait() do
    if shouldLoop2 then
        local args = {
            [1] = "Load",
            [2] = "Layout3"
        }
        game:GetService("ReplicatedStorage").Layouts:InvokeServer(unpack(args))
        wait(1)
        game:GetService("ReplicatedStorage").Rebirth:InvokeServer()
    end
end
