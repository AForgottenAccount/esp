local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/UI-Libraries/main/Vynixius/Source.lua"))()
local Window = Library:AddWindow({
	title = {"Space", "Personal GUI"},
	theme = {
		Accent = Color3.fromRGB(0, 255, 0)
	},
	key = Enum.KeyCode.LeftAlt,
	default = true
})
local Tab = Window:AddTab("Tab", {default = false})
local Section = Tab:AddSection("Player Misc", {default = false})
local Section1 = Tab:AddSection("Teleports", {default = false})
local Section2 = Tab:AddSection("Base Misc", {default = false})
local shouldLoop2 = false

-- Player Misc
local Button = Section:AddButton("120 Fov, Anti AFK", function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/AForgottenAccount/afk120/main/afk%20120", true))()
end)
local Button = Section:AddButton("Claim Daily Reward", function()
	game:GetService("ReplicatedStorage").DailyReward:FireServer()
end)

-- Teleports, setclipboard(tostring(game.Players.LocalPlayer.Character.HumanoidRootPart.Position))
local Button = Section1:AddButton("Teleport to Factory 1", function()
	local teleport_table = {
    location1 = CFrame.new(356.4230651855469, 1368.0511474609375, -381.162353515625)
        }
    local TweenService = game:GetService('TweenService')
    local TweenInfo = TweenInfo.new(2, Enum.EasingStyle.Linear)
    local LocalPlayer = game:GetService('Players').LocalPlayer
    function bypass_teleport(CFrame)
        if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild('HumanoidRootPart') then
            TweenService:Create(LocalPlayer.Character.HumanoidRootPart, TweenInfo, {CFrame = CFrame}):Play()
        end
    end
    bypass_teleport(teleport_table.location1)
end)
